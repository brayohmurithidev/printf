#include <stdarg.h>
#include <unistd.h>
#include "main.h"

/**
 * _printf - prints formated string
 * @format: string to print
 * @...: variadic parameters( unknown)
 * Return: number of characters
 */

int _printf(const char *format, ...)
{
	//expression
	int i = 0;

	while (format[i])
	{
		write(1,&format[i],1);
		i++;
	}
	return (0);
}
